{
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 13,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "overrides": [
        {
            "files": ["**/*.jsx"],
            "rules": {
                "react/prop-types": "off"
            }
        }
    ],
    "rules": {
        "no-unused-vars": 1,
        "no-extra-semi": 1,
        "no-unreachable": 1,
        "no-empty": 1
    }
}